<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineV1Out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.03"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineV2Out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.97"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineH1Out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.03"/>
    <ImageView
        android:id="@+id/logo"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:src="@drawable/instagram_logo"
        app:layout_constraintStart_toStartOf="@+id/guidelineV1Out"
        app:layout_constraintTop_toBottomOf="@id/guidelineH1Out"/>
    <ImageView
        android:id="@+id/share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_share"
        app:layout_constraintEnd_toEndOf="@id/guidelineV2Out"
        app:layout_constraintTop_toBottomOf="@id/guidelineH1Out"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_heart"
        app:layout_constraintEnd_toStartOf="@+id/share"
        app:layout_constraintTop_toBottomOf="@id/guidelineH1Out"
        android:layout_marginEnd="12dp"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guidelineV1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.03"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guidelineV2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.97"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guidelineH2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.095"/>
            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guidelineH3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.22"/>

            <LinearLayout
                android:id="@+id/user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toBottomOf="@id/guidelineH2"
                app:layout_constraintBottom_toTopOf="@id/guidelineH3"
                app:layout_constraintStart_toStartOf="@id/guidelineV1">
                <ImageView
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:src="@drawable/user_story" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_regular"
                    android:text="Cerita Anda"
                    android:layout_gravity="center"
                    android:textColor="@color/black"
                    android:textSize="11sp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/assets_story1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@id/guidelineH2"
                app:layout_constraintBottom_toTopOf="@id/guidelineH3"
                app:layout_constraintStart_toEndOf="@id/user"
                android:layout_marginHorizontal="10dp">
                <ImageView
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:src="@drawable/asset_story" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_regular"
                    android:text="Assets"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    android:textColor="@color/black"
                    android:textSize="11sp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/liga_story1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@id/guidelineH2"
                app:layout_constraintBottom_toTopOf="@id/guidelineH3"
                app:layout_constraintStart_toEndOf="@id/assets_story1"
                android:layout_marginHorizontal="10dp">
                <ImageView
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:src="@drawable/liga_story" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/poppins_regular"
                    android:text="Liga"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="11sp" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/assets_story2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@id/guidelineH2"
                app:layout_constraintBottom_toTopOf="@id/guidelineH3"
                app:layout_constraintStart_toEndOf="@id/liga_story1"
                android:layout_marginHorizontal="10dp">
                <ImageView
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:src="@drawable/asset_story" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_regular"
                    android:text="Assets"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    android:textColor="@color/black"
                    android:textSize="11sp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/liga_story2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@id/guidelineH2"
                app:layout_constraintBottom_toTopOf="@id/guidelineH3"
                app:layout_constraintStart_toEndOf="@id/assets_story2"
                android:layout_marginHorizontal="10dp">
                <ImageView
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:src="@drawable/liga_story" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/poppins_regular"
                    android:text="Liga"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="11sp" />

            </LinearLayout>
            <ImageView
                android:id="@+id/pp_fufufafa"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:src="@drawable/fufufafa_pp"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@id/guidelineH3"
                app:layout_constraintStart_toEndOf="@id/guidelineV1"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                app:layout_constraintStart_toEndOf="@id/pp_fufufafa"
                app:layout_constraintTop_toBottomOf="@id/guidelineH3"
                android:layout_marginTop="16dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fufufafa"
                    android:textColor="@color/black"
                    android:fontFamily="@font/poppins_bold"
                    android:textSize="12sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="putra Mulyono"
                    android:textColor="@color/black"
                    android:fontFamily="@font/poppins_regular"
                    android:textSize="11sp"
                    android:layout_marginTop="-5dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_titik_tiga"
                app:layout_constraintEnd_toStartOf="@id/guidelineV2"
                app:layout_constraintTop_toBottomOf="@id/guidelineH3"
                android:layout_marginTop="24dp"/>
            <ImageView
                android:id="@+id/post_fufufafa"
                android:layout_width="0dp"
                android:layout_height="360dp"
                android:src="@drawable/post_fufufafa"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/pp_fufufafa"
                android:layout_marginTop="16dp"/>
            <ImageView
                android:id="@+id/post_heart1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_heart"
                app:layout_constraintStart_toEndOf="@id/guidelineV1"
                app:layout_constraintTop_toBottomOf="@id/post_fufufafa"
