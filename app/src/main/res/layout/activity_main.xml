                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="tampilkan 10 komentar lainnya"
                android:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:textColor="@color/gray"
                app:layout_constraintTop_toBottomOf="@id/akun_post1"
                app:layout_constraintStart_toEndOf="@id/guidelineV1"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"/>
            <ImageView
                android:id="@+id/pp_assets"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:src="@drawable/assets_ugm_pp"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@id/kalimat_komen1"
                app:layout_constraintStart_toEndOf="@id/guidelineV1"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                app:layout_constraintStart_toEndOf="@id/pp_assets"
                app:layout_constraintTop_toBottomOf="@id/kalimat_komen1"
                android:layout_marginTop="16dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Assets_ugm"
                    android:textColor="@color/black"
                    android:fontFamily="@font/poppins_bold"
                    android:textSize="12sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Sekolah Vokasi Universitas Gadjah Mada"
                    android:textColor="@color/black"
                    android:fontFamily="@font/poppins_regular"
                    android:textSize="11sp"
                    android:layout_marginTop="-5dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_titik_tiga"
                app:layout_constraintEnd_toStartOf="@id/guidelineV2"
                app:layout_constraintTop_toBottomOf="@id/kalimat_komen1"
                android:layout_marginTop="24dp"/>
            <ImageView
                android:id="@+id/post_assets"
                android:layout_width="0dp"
                android:layout_height="360dp"
                android:src="@drawable/post_asset"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/pp_assets"
                android:layout_marginTop="16dp"/>
            <ImageView
                android:id="@+id/post_heart2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_heart"
                app:layout_constraintStart_toEndOf="@id/guidelineV1"
                app:layout_constraintTop_toBottomOf="@id/post_assets"
                android:layout_marginTop="8dp"/>
            <ImageView
                android:id="@+id/post_comment2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_comment"
                app:layout_constraintStart_toEndOf="@id/post_heart2"
                app:layout_constraintTop_toBottomOf="@id/post_assets"
                android:layout_marginHorizontal="5dp"
                android:layout_marginTop="8dp"/>
            <ImageView
                android:id="@+id/post_share2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_share"
                app:layout_constraintStart_toEndOf="@id/post_comment2"
                app:layout_constraintTop_toBottomOf="@id/post_assets"
                android:layout_marginHorizontal="5dp"
                android:layout_marginTop="8dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_bookmark"
                app:layout_constraintEnd_toStartOf="@id/guidelineV2"
                app:layout_constraintTop_toBottomOf="@id/post_assets"
                android:layout_marginTop="8dp"/>
            <TextView
                android:id="@+id/disukai_post2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Disukai oleh Ikhwan_Hanif"
                android:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@id/post_heart2"
                app:layout_constraintStart_toStartOf="@id/guidelineV1"
                android:layout_marginTop="5dp"/>
            <TextView
                android:id="@+id/akun_post2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Assets UGM"
                android:fontFamily="@font/poppins_bold"
                android:textSize="12sp"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@id/disukai_post2"
                app:layout_constraintStart_toStartOf="@id/guidelineV1"
                android:layout_marginTop="5dp"/>
            <TextView
                android:id="@+id/tagar2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#Proud"
                android:fontFamily="@font/poppins_bold"
                android:textSize="12sp"
                android:textColor="@color/blue"
                app:layout_constraintTop_toBottomOf="@id/disukai_post2"
                app:layout_constraintStart_toEndOf="@id/akun_post2"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"/>
            <TextView
                android:id="@+id/kalimat_komen2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="tampilkan 10 komentar lainnya"
                android:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:textColor="@color/gray"
                app:layout_constraintTop_toBottomOf="@id/akun_post2"
                app:layout_constraintStart_toEndOf="@id/guidelineV1"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"/>
            <ImageView
                android:id="@+id/pp_liga"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:src="@drawable/liga_trpl_pp"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@id/kalimat_komen2"
                app:layout_constraintStart_toEndOf="@id/guidelineV1"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                app:layout_constraintStart_toEndOf="@id/pp_liga"
                app:layout_constraintTop_toBottomOf="@id/kalimat_komen2"
                android:layout_marginTop="16dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Liga TRPL"
                    android:textColor="@color/black"
                    android:fontFamily="@font/poppins_bold"
                    android:textSize="12sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Menyambut liga TRPL"
                    android:textColor="@color/black"
                    android:fontFamily="@font/poppins_regular"
                    android:textSize="11sp"
                    android:layout_marginTop="-5dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_titik_tiga"
                app:layout_constraintEnd_toStartOf="@id/guidelineV2"
                app:layout_constraintTop_toBottomOf="@id/kalimat_komen2"
                android:layout_marginTop="24dp"/>
            <ImageView
                android:id="@+id/post_liga"
                android:layout_width="0dp"
                android:layout_height="360dp"
                android:src="@drawable/post_liga"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/pp_liga"
                android:layout_marginTop="16dp"/>
            <ImageView
                android:id="@+id/post_heart3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_heart"
                app:layout_constraintStart_toEndOf="@id/guidelineV1"
                app:layout_constraintTop_toBottomOf="@id/post_liga"
                android:layout_marginTop="8dp"/>
            <ImageView
                android:id="@+id/post_comment3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_comment"
                app:layout_constraintStart_toEndOf="@id/post_heart3"
                app:layout_constraintTop_toBottomOf="@id/post_liga"
                android:layout_marginHorizontal="5dp"
                android:layout_marginTop="8dp"/>
            <ImageView
                android:id="@+id/post_share3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_share"
                app:layout_constraintStart_toEndOf="@id/post_comment3"
                app:layout_constraintTop_toBottomOf="@id/post_liga"
                android:layout_marginHorizontal="5dp"
                android:layout_marginTop="8dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_bookmark"
                app:layout_constraintEnd_toStartOf="@id/guidelineV2"
                app:layout_constraintTop_toBottomOf="@id/post_liga"
                android:layout_marginTop="8dp"/>
